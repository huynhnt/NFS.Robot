                            Height="200"
                            Margin="70,40"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Background="White"
                            CornerRadius="10">
                            <StackPanel Panel.ZIndex="1" Orientation="Vertical">
                                <TextBlock
                                    Margin="0,5"
                                    HorizontalAlignment="Center"
                                    FontFamily="Arial"
                                    FontSize="11"
                                    Foreground="#FF828283"
                                    Text="温度" />
                                <TextBlock
                                    Margin="0,-5"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    FontFamily="Arial"
                                    FontSize="10"
                                    Foreground="#FF828283"
                                    Text="(℃)" />
                                <StackPanel Orientation="Horizontal">
                                    <ProgressBar
                                        Width="160"
                                        Height="8"
                                        Margin="5,10,0,0"
                                        HorizontalAlignment="Left"
                                        Orientation="Vertical"
                                        Value="100">
                                        <ProgressBar.Foreground>
                                            <LinearGradientBrush ColorInterpolationMode="ScRgbLinearInterpolation" MappingMode="Absolute" StartPoint="1,1" EndPoint="100,1">
