                                Margin="10,50,10,0"
                                materialDesign:DataGridAssist.CornerRadius="0"
                                materialDesign:ScrollViewerAssist.CornerRectangleVisibility="Hidden"
                                AutoGenerateColumns="False"
                                Background="#FF414141"
                                BorderBrush="Gray"
                                BorderThickness="0"
                                CanUserAddRows="False"
                                CanUserSortColumns="True"
                                Foreground="White"
                                HeadersVisibility="All"
                                IsReadOnly="True"
                                ItemsSource="{Binding AlarmRankingList, Mode=TwoWay}"
                                ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                ScrollViewer.VerticalScrollBarVisibility="Hidden"
                                SelectionMode="Single">
                                <DataGrid.ColumnHeaderStyle>
                                    <Style TargetType="DataGridColumnHeader">
                                        <Setter Property="FontSize" Value="14" />
                                        <Setter Property="Height" Value="30" />
                                        <Setter Property="Foreground" Value="#FF45B2B8" />
                                        <Setter Property="BorderBrush" Value="#FF5E5D5D" />
                                        <Setter Property="BorderThickness" Value="1" />
                                        <Setter Property="VerticalAlignment" Value="Bottom" />
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="DataGridColumnHeader">
                                                    <Border
                                                        Background="Transparent"
                                                        BorderBrush="White"
                                                        BorderThickness="0"
                                                        CornerRadius="0">
                                                        <ContentPresenter
                                                            Width="Auto"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center" />
                                                    </Border>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                </DataGrid.ColumnHeaderStyle>
                                <DataGrid.Columns>
                                    <DataGridTextColumn
                                        Width="110"
                                        Binding="{Binding Rank}"
                                        EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                                        ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}"
                                        Header="排名" />
                                    <materialDesign:DataGridTextColumn
                                        Width="110"
                                        Binding="{Binding AlertFactor}"
                                        EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnPopupEditingStyle}"
                                        ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}"
                                        Header="告警因子" />
                                    <DataGridTextColumn
                                        Width="110"
                                        Binding="{Binding Location}"
                                        EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                                        ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}"
                                        Header="位置" />
                                    <!--<DataGridTextColumn
                                        Width="110"
                                        Binding="{Binding AlarmsNumber}"
                                        EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnEditingStyle}"
                                        ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}"
                                        Header="告警数" />-->
                                    <DataGridTemplateColumn Width="Auto" Header="告警数">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <StackPanel Orientation="Horizontal">
                                                    <ProgressBar
                                                        x:Name="prgValue"
                                                        Width="72"
                                                        Height="5"
                                                        Margin="0,0,25,0"
                                                        Padding="5,0"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Background="#FF3C3C3C"
                                                        BorderBrush="#FF686868"
                                                        BorderThickness="1"
                                                        FontSize="14"
                                                        Foreground="#FF79B6BA"
                                                        Value="20" />
                                                    <TextBlock
                                                        x:Name="txtAlarmsNumber"
                                                        HorizontalAlignment="Left"
                                                        FontFamily="Arial"
                                                        FontSize="14"
                                                        Foreground="#FF858585"
                                                        Text="{Binding AlarmsNumber}" />
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                        </ScrollViewer>
                    </Grid>
                    <ScrollViewer
                        Grid.Column="2"
                        HorizontalScrollBarVisibility="Hidden"
                        VerticalScrollBarVisibility="Hidden">
                        <Grid Grid.Column="0" Background="#FF414141">
                            <Border
                                Width="510"
                                Height="40"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top">
                                <Border.Background>
                                    <LinearGradientBrush ColorInterpolationMode="ScRgbLinearInterpolation" MappingMode="Absolute" StartPoint="1,1" EndPoint="100,1">
                                        <GradientStop Color="#FF407A7D" />
                                        <GradientStop Offset="0.688" Color="#FF405F61" />
                                        <GradientStop Offset="1.2" Color="#FF3F4646" />
                                    </LinearGradientBrush>
                                </Border.Background>
                                <TextBlock
                                    Margin="15,0"
                                    VerticalAlignment="Center"
                                    FontFamily="Arial"
                                    FontSize="16"
                                    Foreground="#FFF8FAFA"
                                    Text="异常告警占比" />
                            </Border>
                            <Grid Margin="30,50,0,0">
                                <lvc:CartesianChart Height="Auto">
                                    <lvc:CartesianChart.Series>
                                        <lvc:LineSeries
                                            Name="dust6"
                                            Title="噪声(db)"
                                            Fill="Transparent"
                                            LineSmoothness="0"
                                            PointGeometrySize="5"
                                            StrokeThickness="1"
                                            Values="10,20,30,40,50,60,70,80,90" />
                                    </lvc:CartesianChart.Series>
                                    <lvc:CartesianChart.AxisX>
                                        <lvc:Axis
                                            Name="Time6"
                                            Title="时间"
                                            LabelFormatter="{Binding FormatterDust}"
                                            Labels="{Binding EnvNoise.RecordTime}"
                                            MaxValue="24"
                                            MinValue="0"
                                            ShowLabels="True"
                                            Unit="2">
                                            <lvc:Axis.Separator>
                                                <lvc:Separator StrokeThickness="1">
                                                    <lvc:Separator.Stroke>
                                                        <SolidColorBrush Color="#FF928E8E" />
                                                    </lvc:Separator.Stroke>
                                                </lvc:Separator>
                                            </lvc:Axis.Separator>
                                        </lvc:Axis>
                                    </lvc:CartesianChart.AxisX>
                                    <lvc:CartesianChart.AxisY>
                                        <lvc:Axis
                                            Title="百分比"
                                            FontFamily="Arial"
                                            FontSize="12"
                                            LabelFormatter="{Binding DustRecord}"
                                            Labels="{Binding EnvNoise.Record, Mode=TwoWay}"
                                            MaxValue="100"
                                            MinValue="0"
                                            Position="LeftBottom"
                                            ShowLabels="True"
                                            Unit="5">
                                            <lvc:Axis.Separator>
                                                <lvc:Separator StrokeThickness="1">
                                                    <lvc:Separator.Stroke>
                                                        <SolidColorBrush Color="#FF928E8E" />
                                                    </lvc:Separator.Stroke>
                                                </lvc:Separator>
                                            </lvc:Axis.Separator>
                                        </lvc:Axis>
                                    </lvc:CartesianChart.AxisY>
                                </lvc:CartesianChart>
                            </Grid>
                        </Grid>
                    </ScrollViewer>
                </Grid>
            </Grid>
        </Grid>
    </ScrollViewer>
</UserControl>
