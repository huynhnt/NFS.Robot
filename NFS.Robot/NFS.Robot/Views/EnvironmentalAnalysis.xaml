                                                <GradientStop Offset="0.388" Color="#FF77B1C8" />
                                                <GradientStop Offset="0.558" Color="#FFDAB734" />
                                                <GradientStop Offset="0.86" Color="#FFBB5526" />
                                            </LinearGradientBrush>
                                        </ProgressBar.Foreground>
                                    </ProgressBar>
